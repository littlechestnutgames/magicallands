{
  "format_version": "1.16.100",
  "minecraft:entity": {
    "description": {
      "identifier": "glc:gobbler",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": true
    },
    "component_groups": {
      "glc:gobbler_not_activated": {
        "minecraft:loot": {
          "table": "loot_tables/entities/gobbler.json"
        }
      },
      "glc:gobbler_activated": {
        "minecraft:is_chested": {},
        "minecraft:sittable": {
          "sit_event": {"event": "glc:on_gobbler_stay"},
          "stand_event": {"event": "glc:on_gobbler_follow"}
        },
        "minecraft:ambient_sound_interval": {
          "event_name": "ambient.tamed",
          "range": 6.0,
          "value": 16.0
        },
        "minecraft:loot": {
          "table": "loot_tables/entities/gobbler_tamed.json"
        },
        "minecraft:healable": {
          "items": [{
              "item": "apple",
              "heal_amount": 20
            },
            {
              "item": "stick",
              "heal_amount": 5
            }
          ]
        },
        "minecraft:health": {
          "value": 20,
          "max": 20
        },
        "minecraft:breathable": {
          "total_supply": 15,
          "suffocate_time": 0
        },
        "minecraft:leashable": {
          "soft_distance": 4.0,
          "hard_distance": 6.0,
          "max_distance": 10.0
        },
        "minecraft:navigation.walk": {
          "can_path_over_water": true,
          "avoid_damage_blocks": true,
          "can_jump": true
        },
        "minecraft:jump.static": {},
        "minecraft:can_climb": {},
        "minecraft:behavior.float": {
          "priority": 2
        },
        "minecraft:behavior.panic": {
          "priority": 1,
          "speed_multiplier": 1.5
        },
        "minecraft:behavior.share_items": {
          "priority": 9,
          "max_dist": 3,
          "goal_radius": 1.0,
          "speed_multiplier": 0.5,
          "entity_types": [{
            "filters": {
              "any_of": [
                {"test": "is_family", "subject": "other", "value": "fruit"},
                {"all_of": [
                  {"test": "is_family", "subject": "other", "value": "player"},
                  {"test": "is_owner", "subject": "other", "value": true}
                ]}
              ]
            }
          }]
        },
        "minecraft:persistent": {},
        "minecraft:behavior.look_at_player": {
          "priority": 7,
          "look_distance": 6.0,
          "probability": 0.02
        },
        "minecraft:behavior.random_look_around": {
          "priority": 8
        },
        "minecraft:shareables": {
          "all_items": true,
          "all_items_max_amount": 64,
          "all_items_surplus_amount": 1,
          "all_items_want_amount": 64,
          "items": [
            {"item": "cobblestone"}
          ]
        }
      },
      "glc:gobbler_follow": {
        "minecraft:behavior.follow_owner": {
          "priority": 4,
          "stop_distance": 2,
          "start_distance": 8
        },
        "minecraft:behavior.pickup_items": {
          "priority": 1,
          "max_dist": 8,
          "goal_radius": 1,
          "speed_multiplier": 1.5,
          "can_pickup_to_hand_or_equipment": false,
          "can_pickup_any_item": true
        }
      },
      "glc:gobbler_stay": {},
      "minecraft:gobbler_baby": {
        "minecraft:is_baby": {},
        "minecraft:scale": {
          "value": 0.5
        },
        "minecraft:ageable": {
          "duration": 1200,
          "feed_items": [
            "minecraft:stick"
          ],
          "grow_up": {
            "event": "minecraft:ageable_grow_up",
            "target": "self"
          }
        },
        "minecraft:behavior.follow_parent": {
          "priority": 5,
          "speed_multiplier": 1.1
        }
      },
      "minecraft:gobbler_adult": {
        "minecraft:experience_reward": {
          "on_bred": 5,
          "on_death": "query.last_hit_by_player ? 3 : 0"
        },
        "minecraft:breedable": {
          "require_tame": true,
          "breeds_with": {
            "mate_type": "glc:gobbler",
            "baby_type": "glc:gobbler",
            "breed_event": {
              "event": "minecraft:entity_born",
              "target": "baby"
            }
          },
          "breed_items": [
            "minecraft:apple"
          ]
        },
        "minecraft:behavior.breed": {
          "priority": 0,
          "speed_multiplier": 1.3
        }
      }
    },
    "components": {
      "minecraft:type_family": {
        "family": ["magic", "apple", "gobbler", "villager"]
      },
      "minecraft:collision_box": {
        "width": 0.6,
        "height": 0.8
      },
      "minecraft:nameable": {},
      "minecraft:health": {
        "value": 1,
        "max": 1
      },
      "minecraft:movement.basic": {},
      "minecraft:movement": {
        "value": 0.3
      },
      "minecraft:tameable": {
        "probability": 1.0,
        "tame_items": "glc:soulfire_bottle",
        "tame_event": {
          "event": "glc:on_gobbler_tamed",
          "target": "self"
        }
      },
      "minecraft:behavior.tempt": {
        "priority": 4,
        "speed_multiplier": 1.0,
        "items": [
          "query.is_tamed ? glc:soulfire_bottle : stick",
          "query.is_tamed ? glc:soulfire_bottle : apple",
          "glc:soulfire_bottle"
        ]
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [{
          "filters": {
            "test": "in_lava",
            "subject": "self",
            "operator": "==",
            "value": true
          },
          "cause": "lava",
          "damage_per_tick": 4
        }]
      },
      "minecraft:despawn": {
        "despawn_from_distance": {}
      },
      "minecraft:physics": {},
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:conditional_bandwidth_optimization": {},
      "minecraft:inventory": {
        "inventory_size": 1,
        "container_type": "none",
        "private": false,
        "restrict_to_owner": true,
        "can_be_siphoned_from": true,
        "additional_slots_per_strength": 1
      }
    },

    "events": {
      "glc:on_gobbler_tamed": {
        "remove": {
          "component_groups": [
            "glc:gobbler_not_activated"
          ]
        },
        "add": {
          "component_groups": [
            "glc:gobbler_activated",
            "glc:gobbler_follow"
          ]
        }
      },
      "glc:on_gobbler_stay": {
        "remove": {
          "component_groups": [
            "glc:gobbler_follow"
          ]
        },
        "add": {
          "component_groups": [
            "glc:gobbler_stay"
          ]
        },
        "run_command": {
          "command": "particle minecraft:bleach ~ ~1 ~",
          "target": "self"
        }
      },
      "glc:on_gobbler_follow": {
        "add": {
          "component_groups": [
            "glc:gobbler_follow"
          ]
        },
        "remove": {
          "component_groups": [
            "glc:gobbler_stay"
          ]
        },
        "run_command": {
          "command": "particle minecraft:blue_flame_particle ~ ~1 ~",
          "target": "self"
        }
      },
      "minecraft:entity_spawned": {
        "randomize": [{
            "weight": 95,
            "remove": {},
            "add": {
              "component_groups": [
                "minecraft:gobbler_adult",
                "glc:gobbler_not_activated"
              ]
            }
          },
          {
            "weight": 5,
            "remove": {},
            "add": {
              "component_groups": [
                "minecraft:gobbler_baby",
                "glc:gobbler_not_activated"
              ]
            }
          }
        ]
      },

      "minecraft:entity_born": {
        "remove": {},
        "add": {
          "component_groups": [
            "minecraft:gobbler_baby",
            "glc:gobbler_activated"
          ]
        }
      },

      "minecraft:ageable_grow_up": {
        "remove": {
          "component_groups": [
            "minecraft:gobbler_baby"
          ]
        },
        "add": {
          "component_groups": [
            "minecraft:gobbler_adult"
          ]
        }
      }
    }
  }
}